{
	"ConnectionStrings": {
		"LocalSqlServer": "Server=(localdb)\\mssqllocaldb;Database=CleanTemplate;Trusted_Connection=True;MultipleActiveResultSets=true"
	},
	"AllowedHosts": "*",
	"Jwt": {
		"AccessTokenLifetimeMinutes": 10
	},
	"Web": {
		"ApiUrl": "https://api.tacles.net"
	},
	"Identity": {
		"User": {
			"AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@$:+*'",
			"RequireUniqueEmail": true
		},
		"Password": {
			"RequiredLength": 5,
			"RequiredUniqueChars": 1,
			"RequireNonAlphanumeric": false,
			"RequireLowercase": false,
			"RequireUppercase": false,
			"RequireDigit": true
		},
		"SignIn": {
			"RequireConfirmedEmail": false
		}
	},
	"Swagger": {
		"Enabled": false,
		"Title": "CleanTemplate API",
		"Description": "",
		"IncludeComments": [
			"CleanTemplate.WebApi.xml"
		]
	}
}
